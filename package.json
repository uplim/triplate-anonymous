    "next-pwa": "5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.0.1",
    "react-icons": "5.0.1",
    "zod": "3.22.4",
    "zustand": "4.5.0"
