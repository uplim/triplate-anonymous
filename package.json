{
  "name": "triplate-web",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "dev:all": "concurrently -n=nextjs,wiki,storybook -c=green,blue,'#ff528c' \"yarn dev\" \"yarn dev:wiki\" \"yarn dev:storybook\"",
    "dev": "next dev",
    "dev:wiki": "yarn workspace wiki start -p 3001",
    "dev:storybook": "storybook dev -p 6006",
    "start": "next build && next start",
    "lint": "biome check .",
    "lint:fix": "biome check --apply-unsafe .",
    "format": "yarn lint:fix",
    "ci": "biome ci .",
    "test:unit": "jest src/**/*",
    "test:integration": "playwright test tests/integration",
    "test:vrt": "docker compose -f ./dev/vrt/docker-compose.yml run --rm vrt yarn playwright test tests/vrt --project=chromium",
    "test:vrt-update": "yarn test:vrt -u",
    "type-check": "tsc --pretty --noEmit && yarn workspace wiki type-check",
    "build": "next build",
    "build:storybook": "storybook build -o build/storybook"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "0.5.4",
    "client-only": "0.0.1",
    "i18next": "23.10.1",
    "i18next-resources-to-backend": "1.2.0",
    "immer": "10.0.4",
    "negotiator": "0.6.3",
    "next": "14.1.4",
    "next-pwa": "5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.1.0",
    "react-icons": "5.0.1",
    "zod": "3.22.4",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.2",
    "@playwright/test": "1.42.1",
    "@storybook/addon-essentials": "8.0.4",
    "@storybook/addon-interactions": "8.0.4",
    "@storybook/addon-links": "8.0.4",
    "@storybook/blocks": "8.0.4",
    "@storybook/nextjs": "8.0.4",
    "@storybook/react": "8.0.4",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.12",
    "@types/jsdom": "21.1.6",
    "@types/negotiator": "0.6.3",
    "@types/next-pwa": "5.6.9",
    "@types/node": "20.11.30",
    "@types/react": "18.2.70",
    "@types/react-dom": "18.2.22",
    "concurrently": "8.2.2",
    "cross-env": "7.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-fail-on-console": "3.1.2",
    "storybook": "8.0.4",
    "storybook-addon-pseudo-states": "3.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.4.3"
  },
  "engines": {
    "node": "20.11.1",
    "npm": "use yarn instead",
    "yarn": "4.1.1"
  },
  "workspaces": [
    "wiki"
  ]
}
