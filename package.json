{
  "name": "triplate-anonymous",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "dev": "scripts/start-firebase-emulator.sh && cross-env FIRESTORE_EMULATOR_HOST=localhost:8080 FIREBASE_STORAGE_EMULATOR_HOST=localhost:9199 next dev && ./scripts/stop-firebase-emulator.sh",
    "start": "next start",
    "lint": "biome check .",
    "lint:fix": "biome check --apply-unsafe .",
    "format": "biome format --write .",
    "ci": "biome ci .",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit",
    "storybook": "storybook dev -p 6006",
    "build": "next build",
    "build:storybook": "storybook build -o build/storybook"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "0.5.4",
    "client-only": "0.0.1",
    "firebase-admin": "12.0.0",
    "i18next": "23.7.16",
    "i18next-resources-to-backend": "1.2.0",
    "immer": "10.0.3",
    "negotiator": "0.6.3",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.0.0",
    "react-icons": "5.0.1",
    "zod": "3.22.4",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.2",
    "@storybook/addon-essentials": "7.6.9",
    "@storybook/addon-interactions": "7.6.9",
    "@storybook/addon-links": "7.6.9",
    "@storybook/blocks": "7.6.9",
    "@storybook/nextjs": "7.6.9",
    "@storybook/react": "7.6.9",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.11",
    "@types/jsdom": "21.1.6",
    "@types/negotiator": "0.6.3",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "cross-env": "7.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-fail-on-console": "3.1.2",
    "storybook": "7.6.9",
    "storybook-addon-pseudo-states": "2.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": "20.11.0",
    "npm": "use yarn instead",
    "yarn": "4.0.2"
  }
}
